{
  "name": "grievances",
  "version": "0.1.1",
  "private": true,
  "jest": {
    "setupEnvScriptFile": "./node_modules/react-native/jestSupport/env.js",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-addons-test-utils",
      "react-native-router-flux",
      "promise",
      "source-map",
      "key-mirror",
      "immutable",
      "fetch",
      "redux",
      "redux-thunk",
      "fbjs"
    ],
    "collectCoverage": false,
    "verbose": true
  },
  "scripts": {
    "test": "rm -rf ./node_modules/jest-cli/.haste_cache && jest  --no-cache",
    "start": "react-native start",
    "test:watch": "npm run test -- --watch",
    "test-chrome": "node-debug --preload false --nodejs --harmony jest --runInBand",
    "docs": "./node_modules/docker/docker -w -i src/ -o ../grievances-pages "
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "key-mirror": "^1.0.1",
    "native-base": "^2.1.0",
    "querystring": "^0.2.0",
    "react": "^15.5.3",
    "react-native": "^0.43.3",
    "react-native-code-push": "^1.17.2-beta",
    "react-native-config": "^0.3.1",
    "react-native-fbsdk": "^0.5.0",
    "react-native-gifted-spinner": "^0.1.0",
    "react-native-google-places-autocomplete": "^1.2.11",
    "react-native-google-signin": "^0.9.0",
    "react-native-i18n": "^1.0.0",
    "react-native-image-picker": "^0.26.2",
    "react-native-maps": "^0.14.0",
    "react-native-navbar": "^1.7.1",
    "react-native-router-flux": "^3.38.0",
    "react-native-simple-store": "^1.1.0",
    "react-native-simpledialog-android": "^1.0.8",
    "react-native-swiper": "^1.5.4",
    "react-native-vector-icons": "^4.0.0",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "regenerator": "^0.9.7",
    "tcomb-form-native": "^0.6.7",
    "underscore": "^1.8.3",
    "validate.js": "^0.11.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-preset-react-native": "^1.9.1",
    "docker": "^1.0.0",
    "istanbul": "^0.4.5",
    "jest-cli": "^19.0.2",
    "react-addons-test-utils": "^15.5.0",
    "redux-mock-store": "^1.2.3",
    "rnpm-plugin-upgrade": "^0.26.0"
  }
}
